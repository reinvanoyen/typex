{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "commands": [
    {
      "name": "Export text styles as HTML fontbook",
      "shortcut": "ctrl shift f",
      "identifier": "html-fontbook-export",
      "script": "html-fontbook-export.js"
    },
    {
      "name": "Export text styles as JSON",
      "shortcut": "ctrl shift j",
      "identifier": "json-export",
      "script": "json-export.js"
    },
    {
      "name": "Export text styles as CSS classes",
      "shortcut": "ctrl shift c",
      "identifier": "css-export",
      "script": "css-export.js"
    },
    {
      "name": "Export text styles as SASS mixins",
      "shortcut": "ctrl shift s",
      "identifier": "sass-mixins-export",
      "script": "sass-mixins-export.js"
    }
  ],
  "menu": {
    "title": "Typex",
    "items": [
      "html-fontbook-export",
      "json-export",
      "css-export",
      "sass-mixins-export"
    ]
  },
  "version": "0.4.0",
  "description": "Highly configurable text styles to web (css, sass, html, json, ...) export plugin",
  "name": "typex",
  "identifier": "typex",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/reinvanoyen/typex/master/.appcast.xml"
}